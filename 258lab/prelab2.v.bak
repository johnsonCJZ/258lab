module mux(LEDR, SW);
    input [9:0] SW;
    output [9:0] LEDR;
	 wire Connection1
	 wire Connection2
	 
    mux2to1 b0(
        .x(SW[0]),
        .y(SW[1]),
        .s(Connection1),
        );
	
    mux2to1 b1(
        .x(SW[2]),
        .y(SW[3]),
        .s(Connection2),
        );
		  
    mux2to1 b3(
        .x(Connection1),
        .y(Connection2),
        .s(LEDR[0]),
        );
endmodule

module mux2to1(x, y, s, m);
    input x; //selected when s is 0
    input y; //selected when s is 1
    input s; //select signal
    output m; //output
  
    assign m = s & y | ~s & x;
    // OR
    // assign m = s ? y : x;

endmodule
